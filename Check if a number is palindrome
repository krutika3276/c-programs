#include <stdio.h>

int main() {
    int num, real, reverse = 0, digit;

    printf("Enter a number: ");
    scanf("%d", &num);

    real = num;  

    while(num > 0) {
        digit = num % 10;             
        reverse = reverse * 10 + digit; 
        num = num / 10;               
    }

    if(real == reverse)
        printf("%d is a palindrome number\n", real);
    else
        printf("%d is not a palindrome number\n", real);

    return 0;
}
